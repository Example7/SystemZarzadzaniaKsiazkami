@model IList<AppData.Data.CMS.Sponsor>

<div style="background-image: linear-gradient(to right, #ff7e5f, #feb47b);">
    <div class="container pt-3 pb-0">
        <div class="row">
            @foreach (var sponsor in Model)
            {
                <div class="col-6 col-md-4 col-lg-2 mb-4">
                    <img src="@sponsor.LogoUrl" alt="@sponsor.Nazwa" class="img-fluid mx-auto d-block" style="width: 150px; height: 150px; object-fit: contain;" />
                </div>
            }
        </div>
    </div>
</div>
